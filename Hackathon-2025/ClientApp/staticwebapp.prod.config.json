{
  "routes": [
    {
      "route": "/sitemap.xml",
      "rewrite": "https://api.starlitstories.app/sitemap.xml"
    },
    {
      "route": "/sitemaps/*",
      "rewrite": "https://api.starlitstories.app/sitemaps/{*}"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/images/*",
      "/favicon*",
      "/robots.txt",
      "/sitemap.xml",
      "/sitemaps/*"
    ]
  },
  "mimeTypes": {
    ".js": "text/javascript"
  }
}
