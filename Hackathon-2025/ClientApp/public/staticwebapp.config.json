{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "/assets/*",
      "/*.js",
      "/*.css",
      "/favicon*",
      "/images/*"
    ]
  },
  "routes": [
    {
      "route": "/login",
      "rewrite": "/index.html"
    },
    {
      "route": "/signup",
      "rewrite": "/index.html"
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff",
    "Referrer-Policy": "strict-origin-when-cross-origin"
  }
}
